FUNCTION "XSK_SAMPLES_HDB_TABLEFUNCTION_SIMPLE"."hdb-tablefunction-simple.db::ItemTableFunction" ()
	RETURNS TABLE (
		"Id" NVARCHAR(32),
		"Status" NVARCHAR(32)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS

BEGIN

	RETURN  SELECT "Id", "Status" FROM "hdb-tablefunction-simple.db::ItemForTableFunction" WHERE (UPPER(SESSION_USER) = 'TESTUSER');

END;