namespace gstr2;

@Schema: 'DBADMIN'

context ViewDefinitionNested {

      context embedded_context {
           view embedded_view as select from ViewDefinitionNested.employees {
                 ID,
                 NAME
           };
      }

      entity employees {
          key ID       : String(32);
          NAME         : String(500);
          ADDRESS      : String(5000);
          AGE          : Integer;
          PHONE        : String(200);
      };

}